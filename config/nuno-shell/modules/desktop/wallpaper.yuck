(defwindow wallpaper
    :monitor 0
    :stacking "bg"
    :windowtype "desktop"
    :wm-ignore true
    :geometry (geometry
        :x "-60px"
        :width "100%"
        :height "100%"
    )
    (wallpaperWidget)
)

(defwidget wallpaperWidget []
    (box
        :class "wallpaperWidget"
    )
)
